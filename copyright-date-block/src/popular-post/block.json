{
	"$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
	"name": "wda/popular-post",
	"title": "Custom Popular Post",
	"category": "text",
	"attributes": {
		"heading": {
			"type": "string",
			"default": "Popular Post"
		},
		"headingAlign": {
			"type": "string",
			"default": "left"
		}
	},
	"styles": [
		{
			"name": "popular-post-grid-view",
			"label": "Grid View",
			"isDefault": true
		},
		{
			"name": "popular-post-list-view",
			"label": "List View"
		}
	],
	"editorScript": "file:./index.js",
	"render": "file:./render.php",
	"style": "file:./style-index.css",
	"supports": {
		"align": true
	}
}
